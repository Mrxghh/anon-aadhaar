    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.20",
    "circomlib": "^2.0.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "node-forge": "^1.3.1",
    "prettier": "^2.8.8",
    "sinon": "^17.0.1",
    "ts-mocha": "^10.0.0",
