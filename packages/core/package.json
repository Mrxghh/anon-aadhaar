    "@zk-email/helpers": "^3.1.3",
    "buffer": "^6.0.3",
    "json-bigint": "^1.0.0",
    "localforage": "^1.10.0",
    "pako": "^2.1.0",
    "snarkjs": "^0.7.3",
    "uuid": "^9.0.0",
    "valid-url": "^1.0.9",
    "web-worker": "1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
