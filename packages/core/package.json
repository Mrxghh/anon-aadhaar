  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "npx ts-mocha test/**/*.test.ts --exit",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "cd ../.. && husky install",
    "test-publish": "yarn build && yarn lint && yarn test && npm publish --dry-run",
    "publishtonpm": "yarn build && yarn lint && npm publish",
    "bump-to": "yarn version --no-commit-hooks --no-git-tag-version --new-version"
  },
  "files": [
    "./artifacts/verification_key.json",
    "./src/*",
    "./dist/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@pcd/pcd-types": "^0.10.0",
